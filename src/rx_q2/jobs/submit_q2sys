#!/bin/bash

source $HOME/.bashrc
conda_init
conda activate rk 

IJOB=$1
VERS=$2
SYST=$3
TRIG=$4
YEAR=$5
BREM=$6

CACHE_NAME=settings_$IJOB.txt

print_q2_settings -i $IJOB -t $TRIG -d $YEAR -b $BREM > $CACHE_NAME 

TRIG=$(cat $CACHE_NAME | grep TRIG | awk -F '=' '{print$2}')
YEAR=$(cat $CACHE_NAME | grep YEAR | awk -F '=' '{print$2}')
BREM=$(cat $CACHE_NAME | grep BREM | awk -F '=' '{print$2}')

echo "$VERS, $YEAR, $TRIG, $BREM, $SYST"
get_q2_tables -v $VERS -y $YEAR -t $TRIG -b $BREM -x $SYST

